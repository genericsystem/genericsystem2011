<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
						http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">

	<application>
		<resource-bundle>
			<base-name>/bundles/messages</base-name>
			<var>msg</var>
		</resource-bundle>
		<resource-bundle>
			<base-name>/bundles/infos</base-name>
			<var>infos</var>
		</resource-bundle>
	</application>

	<converter>
		<converter-id>genericConverter</converter-id>
		<converter-class>org.genericsystem.myadmin.beans.converters.GenericConverter</converter-class>
	</converter>

</faces-config>