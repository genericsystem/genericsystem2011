<?xml version="1.0" encoding="UTF-8"?>
<ui:composition xmlns="http://www.w3.org/1999/xhtml"
	xmlns:h="http://java.sun.com/jsf/html"
	xmlns:f="http://java.sun.com/jsf/core"
	xmlns:c="http://java.sun.com/jsp/jstl/core"
	xmlns:ui="http://java.sun.com/jsf/facelets">
	<h:form>
		<h:dataTable value="#{genericBean.children(component)}" var="row">
			<f:facet name="header">
				<h:commandButton value="UP"
					action="#{genericBean.changeComponent(genericBean.parent)}" rendered="#{!genericBean.genericSelected.isEngine()}" />
				<h:outputText value="Manage #{genericBean}" />
			</f:facet>
			<h:column>
				<h:outputText value="#{row}" />
			</h:column>
			<h:column>
				<h:commandButton value="#{genericBean.editMsg}"
					action="#{genericBean.changeComponent(row)}" />
			</h:column>
			<h:column>
				<h:commandButton value="#{genericBean.removeMsg}"
					action="#{genericBean.remove(row)}" />
			</h:column>
			<f:facet name="footer">
				<h:inputText value="#{genericBean.newValue}" />
				<h:commandButton value="#{genericBean.addMsg}"
					action="#{genericBean.add}" />
			</f:facet>
		</h:dataTable>
	</h:form>

</ui:composition>